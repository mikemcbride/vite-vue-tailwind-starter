{
    "name": "vite-vue-tailwind-starter",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "jest src",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.17.12",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/vue": "^6.4.2",
        "@vitejs/plugin-vue": "^2.3.3",
        "autoprefixer": "^10.4.7",
        "babel-jest": "^26.6.3",
        "jest": "^26.6.3",
        "postcss": "^8.4.14",
        "tailwindcss": "^3.0.24",
        "vite": "^2.9.9",
        "vue-jest": "^5.0.0-alpha.10"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts",
            "tsx",
            "vue"
        ],
        "transform": {
            "^.+\\.(js|ts|tsx)$": "babel-jest",
            "^.+\\.vue$": "vue-jest"
        },
        "testMatch": [
            "**/*.test.+(js|ts|tsx)"
        ]
    }
}
